#compdef _tblastn

local args
args+=(
	'(-culling_limit)-best_hit_overhang[Best Hit algorithm overhang value]:float_value: '
	'(-culling_limit)-best_hit_score_edge[Best Hit algorithm score edge value]:float_value: '
